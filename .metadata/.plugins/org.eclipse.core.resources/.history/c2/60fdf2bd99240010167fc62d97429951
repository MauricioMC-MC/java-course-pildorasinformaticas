
public class UsoArrays {

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub

		
		int times = 5;
		int [] myArray = new int[times];
		
		
		myArray[0]=5;
		myArray[1]=10;
		myArray[2]=15;
		myArray[3]=20;
		myArray[4]=25;
		
		for (int i=0; i<times; i++)
		{
			System.out.println(i);
		}
		
	}

}
